	file = {Full Text PDF:/Users/annatwater/Zotero/storage/ULVX2B2C/Bang et al. - 2023 - Access and Exposure to Local News Media in the Dig.pdf:application/pdf},
}

@misc{fajgelbaum_political_2023,
